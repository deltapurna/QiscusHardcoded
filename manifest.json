{
  "manifest_version": 2,

  "name": "Qiscus Hardcoded",
  "description": "Qiscus chrome extension - hardcoded",
  "version": "1.0",
  "permissions": [    
    "https://qisc.us/api/v1/mobile/",
    "https://qiscus-staging.herokuapp.com/"
  ],
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "background": {
    "scripts": ["js/vendor/angularjs/angular.min.js"]
  },
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "popup.html"
  }
}
